{% extends "layout.njk" %}

{% block main %}
{% for definedType in definedTypesToExport %}
{% if definedType.type.kind == 'structTypeNode' %}
pub mod {{ definedType.name | snakeCase }}_schema;
{% elif definedType.type.kind == 'enumTypeNode' %}
pub mod {{ definedType.name | snakeCase }}_schema;
{% elif definedType.type.kind != 'structTypeNode' and definedType.type.kind != 'enumTypeNode' %}
pub mod {{ definedType.name | snakeCase }}_schema;
{% endif %}
{% endfor %}

{% for definedType in definedTypesToExport %}
{% if definedType.type.kind == 'structTypeNode' %}
pub use {{ definedType.name | snakeCase }}_schema::*;
{% elif definedType.type.kind == 'enumTypeNode' %}
pub use {{ definedType.name | snakeCase }}_schema::*;
{% elif definedType.type.kind != 'structTypeNode' and definedType.type.kind != 'enumTypeNode' %}
pub use {{ definedType.name | snakeCase }}_schema::*;
{% endif %}
{% endfor %}
{% endblock %}