{
    "name": "@sevenlabs-hq/carbon-codama-renderer",
    "version": "0.1.2",
    "description": "Carbon codama renderer for Codama IDL",
    "keywords": [
        "solana",
        "rust",
        "carbon",
        "codama",
        "renderer",
        "client"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/sevenlabs-hq/carbon"
    },
    "homepage": "https://github.com/sevenlabs-hq/carbon#readme",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist/**",
        "templates/**"
    ],
    "scripts": {
        "build": "tsup && cp -r templates dist/",
        "dev": "tsup --watch",
        "type-check": "tsc --noEmit",
        "lint": "echo 'No linting configured'",
        "test": "echo 'No tests configured'",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "@codama/errors": "1.3.0",
        "@codama/nodes": "1.3.0",
        "@codama/renderers-core": "1.0.16",
        "@codama/visitors-core": "1.3.0",
        "@solana/codecs-strings": "^2.2.0",
        "nunjucks": "^3.2.4"
    },
    "devDependencies": {
        "@types/nunjucks": "^3.2.6",
        "rimraf": "^6.0.1",
        "tsup": "^8.5.0",
        "typescript": "^5.8.3"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
